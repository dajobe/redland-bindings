library project Librdf is

   for Library_Name use "rdf-ada";
   for Library_Dir use "lib";
   for Library_Ali_Dir use "ali";
   for Library_Kind use External ("LIBRARY_KIND");
   for Library_Version use External ("soversion");

   package Compiler is
      for Default_Switches ("ada") use ("-g", "-O2", "-gnat2012");
   end Compiler;

   for Source_Dirs use ("src");
   for Object_Dir use "obj";

end Librdf;

